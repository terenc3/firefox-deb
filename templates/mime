text/html; %bin% %s; description=HTML Text; test=test -n "$DISPLAY";  nametemplate=%s.html; priority=5
text/xml; %bin% %s; description=XML Text; test=test -n "$DISPLAY";  nametemplate=%s.xml; priority=5
image/png;  %bin% %s; description=PNG Image; test=test -n "$DISPLAY";  nametemplate=%s.png; priority=2
image/jpeg;  %bin% %s; description=JPEG Image; test=test -n "$DISPLAY";  nametemplate=%s.jpeg; priority=2
image/gif;  %bin% %s; description=GIF Image; test=test -n "$DISPLAY";  nametemplate=%s.gif; priority=2
